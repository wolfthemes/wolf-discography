"<br><br>\n"
"\t\t\t\t\t<a href=\"%1$s\" class=\"button button-primary\">Create a page</"
"a>\n"
"\t\t\t\t\t&nbsp;\n"
"\t\t\t\t\t<a href=\"%2$s\" class=\"button button-primary\">Select an "
"existing page</a>\n"
"\t\t\t\t\t&nbsp;\n"
"\t\t\t\t\t<a href=\"%3$s\" class=\"button\">Skip setup</a>"
msgstr ""
"<br><br>\n"
"\t\t\t\t\t<a href=\"%1$s\" class=\"button button-primary\">Crea una pagina</"
"a>\n"
"\t\t\t\t\t \n"
"\t\t\t\t\t<a href=\"%2$s\" class=\"button button-primary\">Selezionare una "
"pagina esistente</a>\n"
